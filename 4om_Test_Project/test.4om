set-prv-mcs {{
            (def-prv 
                (def-prv ~n ~f ~p) 
                ({set-prv-mcs 
                    {{
                        (~n ~f ~p)
                    }}
                })
            )
            [unw [unw [prv-mcs]]]
        }}

def-prv f-is (~a is ~b) ((False))
def-prv t-is (~a is ~a) ((True))